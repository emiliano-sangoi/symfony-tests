{% extends 'base.html.twig' %}

{% block body %}
    <h1>Domicilio</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ domicilio.id }}</td>
            </tr>
            <tr>
                <th>Calle</th>
                <td>{{ domicilio.calle }}</td>
            </tr>
            <tr>
                <th>Numero</th>
                <td>{{ domicilio.numero }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('forms_ejemplo2_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('forms_ejemplo2_edit', { 'id': domicilio.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
